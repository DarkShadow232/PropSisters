# PropSisters Frontend Environment Variables

# API Configuration
VITE_API_URL=http://localhost:3000/api
VITE_FRONTEND_URL=http://localhost:8081

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/test

# Development Configuration
VITE_NODE_ENV=development
VITE_DEBUG_MODE=true

# Feature Flags
VITE_ENABLE_GOOGLE_AUTH=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_CACHING=true

# App Configuration
VITE_APP_NAME=PropSisters
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Property Rental Platform

# Timezone and Currency
VITE_DEFAULT_TIMEZONE=Africa/Cairo
VITE_DEFAULT_CURRENCY=EGP

# Pagination
VITE_DEFAULT_PAGE_SIZE=10
VITE_MAX_PAGE_SIZE=100

# File Upload
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_FILE_TYPES=jpg,jpeg,png,gif,webp

# Cache Settings
VITE_CACHE_TTL=3600
VITE_CACHE_MAX_SIZE=1000

# Email Configuration
VITE_ADMIN_EMAIL=admin@propsisters.com
VITE_SUPPORT_EMAIL=support@propsisters.com
