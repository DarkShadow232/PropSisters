# Environment Variables Template for Sisterhood Style Rentals
# Copy this file to .env.local and fill in your actual values

# Firebase Configuration (Required)
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Application Settings
VITE_APP_NAME="Sisterhood Style Rentals"
VITE_APP_URL=http://localhost:8081
NODE_ENV=development

# API Configuration
VITE_API_BASE_URL=http://localhost:8081/api

# Development Settings
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug

# External Services (Optional)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
VITE_ANALYTICS_ID=your_analytics_id
VITE_STRIPE_PUBLIC_KEY=your_stripe_public_key
VITE_PAYPAL_CLIENT_ID=your_paypal_client_id

# Email Service (Optional)
VITE_EMAIL_SERVICE_ID=your_email_service_id
VITE_EMAIL_TEMPLATE_ID=your_email_template_id
VITE_EMAIL_PUBLIC_KEY=your_email_public_key

# Social Media Links
VITE_FACEBOOK_URL=https://facebook.com/your_page
VITE_INSTAGRAM_URL=https://instagram.com/your_account
VITE_TWITTER_URL=https://twitter.com/your_account
VITE_WHATSAPP_NUMBER=+1234567890

# Contact Information
VITE_CONTACT_EMAIL=info@yourdomain.com
VITE_CONTACT_PHONE=+1234567890
VITE_SUPPORT_EMAIL=support@yourdomain.com

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_CHAT_SUPPORT=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_REVIEWS=true
VITE_ENABLE_BOOKING=true